{
  "name": "react-things-happen-app",
  "homepage": "http://lena-ts.github.io/react-things-happen-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "build:css": "postcss src/styles/index.scss -o src/index.css",
    "watch:css": "postcss src/styles/index.scss -o src/index.css -w",
    "start": "npm-run-all -p watch:css start-js",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "build": "npm-run-all build:css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": "Lena Tsybulenko <lena.tsybulenko@gmail.com>",
  "devDependencies": {
    "add": "^2.0.6",
    "autoprefixer": "^9.8.0",
    "gh-pages": "^3.1.0",
    "next": "^9.4.2",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "postcss-nested": "^4.2.1",
    "react-day-picker": "^7.4.8",
    "react-transition-group": "1.x",
    "yarn": "^1.22.4"
  }
}
